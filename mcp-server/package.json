  "license": "GPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "https://github.com/anthropics/telebiz-mcp"
  },
  "homepage": "https://github.com/anthropics/telebiz-mcp#readme",
  "bugs": {
    "url": "https://github.com/anthropics/telebiz-mcp/issues"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/ws": "^8.5.13",
    "tsx": "^4.20.0",
