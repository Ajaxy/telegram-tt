    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@cryptography/aes": "^0.1.1",
    "async-mutex": "^0.3.2",
    "big-integer": "github:painor/BigInteger.js",
    "croppie": "^2.6.5",
    "emoji-data-ios": "git+https://github.com/korenskoy/emoji-data-ios#3c401da02c6792cea0b6c758795da406377bf667",
    "events": "^3.3.0",
